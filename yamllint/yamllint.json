{
    "problemMatcher": [
        {
            "owner": "yamlint",
            "pattern": [
                {
                    "regexp": "^(.+)\\:(\\d+)\\:(\\d+)\\:\\s+\\[(.+)\\]\\s+(.+)\\s+\\((.+)\\)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }
    ]
}